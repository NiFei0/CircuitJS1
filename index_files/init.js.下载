$(document).ready(function(){1==set.module.imglightbox&&($(".post-content-content img").not(".c-downbtn-icon,.cp-postcard-item img,.wxshow-img-plane img").each(function(){$(this).attr("data-fancybox","gallery");1==set.module.imglazyload&&$(this).attr("data-src",$(this).attr("data-original"))}),Fancybox.bind("[data-fancybox\x3dgallery]",{backFocus:!1,Toolbar:{display:["zoom","slideshow","fullscreen","close","counter"]},Thumbs:{autoStart:!1}}))});